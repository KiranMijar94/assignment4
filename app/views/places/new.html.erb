<% if @current_user %>
  <h1>New Place</h1>

  <%= form_with :model => @place do |form| %>
    <p>
      <%= form.label "name" %>
      <%= form.text_field "name" %>
    </p>
    <p>
      <%= form.submit :class => "btn btn-primary btn-lg" %>
    </p>
  <% end %>
<% else %>
  <% controller.redirect_to "/sessions/new"%>
<% end %>